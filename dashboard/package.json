		"@testing-library/svelte": "^5.2.8",
		"@tailwindcss/vite": "^4.1.18",
		"jsdom": "^26.0.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"lucide-svelte": "^0.562.0",
		"tailwind-merge": "^3.4.0"
